{
  "name": "blockatlas",
  "buildpacks":[
    {
      "url":"heroku/go"
    }
  ],
  "addons": [
    "heroku-postgresql:hobby-dev",
    "cloudamqp"
  ],
  "environments": {
    "review": {
      "addons": [
        {
          "plan":"heroku-postgresql:hobby-dev",
          "as":"POSTGRES"
        },
        {
          "plan":"heroku-postgresql:hobby-dev",
          "as":"POSTGRES_READ"
        },
        {
          "plan":"cloudamqp:lemur",
          "as":"OBSERVER_RABBITMQ"
        }
      ]
    }
  }
}
