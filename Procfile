web: bin/api -c $HOME/config.yml -p $PORT
consumer-transactions: CONSUMER_SERVICE=transactions bin/consumer -c $HOME/config.yml
consumer-subscriptions: CONSUMER_SERVICE=subscriptions bin/consumer -c $HOME/config.yml
consumer-tokens: CONSUMER_SERVICE=tokens bin/consumer -c $HOME/config.yml
parser: bin/parser -c $HOME/config.yml
